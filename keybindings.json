// 將您的按鍵組合放入此檔案中以覆寫預設值
[
    {
        "key": "ctrl+alt+space",
        "command": "editor.action.indentationToSpaces"
    },
    {
        "key": "ctrl+alt+f",
        "command": "editor.action.reindentlines"
    },
    {
        "key": "shift+alt+space",
        "command": "editor.action.indentUsingSpaces"
    }
]